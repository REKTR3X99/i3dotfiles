URxvt.font: xft:Source Code Pro:size=14
!URxvt.font : xft:Hack-Regular:Size =  14
!URxvt*font : xft:monospace:Regular:size=13
!URxvt.font:xft:inconsolata-regular:size=15

URxvt.dpi : 96
URxvt.scrollBar : false
URxvt.scrollBar_floating : true
URxvt.scrollBar_right : true
URxvt.scrollColor : #3f3f3f
URxvt.cursorUnderline : true

!URxvt.tintColor : #ffffff
!URxvt.transparent : true
!URxvt.blur : 50
!URxvt.blurRadius : 7
!URxvt.depth : 16
!URxvt.shading : 3
URxvt.saveLines : 100000
URxvt.highlightColor : rgb:FF/69/B4

URxvt.cursorBlink : true
URxvt.fading : 1.0
URxvt.urlLauncher : chromium
URxvt.externalBorder : 1

!URxvt.BorderColor : rgb:AA/AA/AA
!URxvt.cursorColor : rgb:ff/ff/ff

